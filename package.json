{
  "private": true,
  "scripts": {
    "schema": "prisma generate --watch",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "email": "docker compose up -d email && sleep 1 && open http://localhost:4444/",
    "clean": "rm -rf prisma/migrations",
    "dev": "pnpm schema & next dev & (sleep 3 && open http://localhost:3000) & (sleep 2 && pnpm studio)",
    "dx": "pnpm dev",
    "up": "pnpm clean && pnpm email && sleep 2 && pnpm newdb && pnpm dx",
    "down": "docker compose down && rm -rf data",
    "studio": "prisma studio",
    "migrate": "prisma migrate dev",
    "newdb": "pnpm migrate && yarn seed",
    "seed": "prisma db seed",
    "go": "yarn down && yarn up",
    "postinstall": "prisma generate",
    "generate": "prisma generate",
    "prepare": "husky install"
  },
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "ts-node prisma/seed.ts"
  },
